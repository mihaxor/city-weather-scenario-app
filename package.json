{
  "name": "simple-scenario-app",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "local": "open http://localhost:3000 && vite",
    "dev": "vite",
    "build:client": "tsc -p tsconfig.json && vite build",
    "build:server": "tsc -p tsconfig.node.json",
    "build": "yarn build:client && yarn build:server",
    "server": "node server/dist/index.js",
    "eject": "vite eject",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:inspector": "eslint --inspect-config",
    "preview": "vite preview"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@reduxjs/toolkit": "^2.8.2",
    "better-sqlite3": "^12.2.0",
    "bootstrap": "^5.3.7",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "debug": "^4.4.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.1",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.7.1"
